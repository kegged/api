{
  "presets": [
    ["env", {
      "targets": {
        "node": "current"
      }
    }],
    "stage-0"
  ],
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@": "./src",
        "~": "./test"
      }
    }]
  ]
}
